// Create a new function called 'test'
func test() {
    print("Called test")
}

// Create a new function called 'test2' that takes in an int and returns an int
func test2(int x) -> int {
    return x + 100
}

// Start with a main function that returns an int
func main() -> int {
    test() // Call 'test'
    print(test2(100)) // Call 'test2'

    return 0 // Return 0 to indicate success
}
